BLYNK_WRITE(V10) //writes the data to virtual pin V10
{
int i=param.asInt();
digitalWrite(13,LOW);
digitalWrite(12,LOW);
switch(i)
{
  case 0:
  {
    digitalWrite(13,HIGH);
    digitalWrite(12,LOW);
    break;
  }
  case 1:
  {
    digitalWrite(13,HIGH);
    delay(10000);
    digitalWrite(12,HIGH);
    delay(10000);
    break;
  }
  case 2:
  {
    digitalWrite(13,HIGH);
